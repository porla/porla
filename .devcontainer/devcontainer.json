{
    "name": "Porla dev",
    "build": {
        "dockerfile": "./Dockerfile",
        "context": ".."
    },
    "forwardPorts": [
        1337
    ],
    "mounts": [],
    "remoteUser": "vscode",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-azuretools.vscode-docker",
                "ms-vscode.cpptools-extension-pack"
            ],
            "settings": {
                "terminal.integrated.shell.linux": "/bin/zsh"
            }
        }
    }
}